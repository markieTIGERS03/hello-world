openssl enc -aes-128-cbc -a -salt -pass pass:asdffdsa -kfile cred.txt


#!/bin/bash

read -p "Enter Code" hcode

var1=$(cat cred.txt | openssl enc -aes-128-cbc -a -d -salt -pass pass:$hcode)

echo "$var1"
